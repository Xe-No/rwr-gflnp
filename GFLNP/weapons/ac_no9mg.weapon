<?xml version="1.0" encoding="utf-8"?>
<!-- <NO.9 光束枪模式/>-->
<weapons>
	<weapon file="base_resupply.weapon" key="ac_no9mg.weapon">
		<tag name="machine gun"/>
		<specification accuracy_factor="1.1" barrel_offset_3d="0 0 1.5" can_shoot_crouching="1" can_shoot_prone="0" can_shoot_standing="1" carry_in_two_hands="0" class="0" magazine_size="-1" name="VE66-LRA" projectile_speed="220.0" projectiles_per_shot="1" retrigger_time="0.06" sight_range_modifier="1.2" slot="0" spread_range="0.001" stab_enabled="1" stab_range="8" suppressed="0" sustained_fire_diminish_rate="0.45" sustained_fire_grow_step="0.5"/>
		<drop_on_death_result class="spawn" direction_spread="0 0" instance_class="projectile" instance_key="ugv_blast.projectile" max_amount="1" min_amount="1" offset="0 0 0" position_spread="0 0"/>
		<next_in_chain key="ac_401blade.weapon" share_ammo="0"/>
