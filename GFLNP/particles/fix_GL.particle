particle_system fix_range
{
	quota 100
	emit_emitter_quota 3
	material Examples/Aureola
	particle_width 30
	particle_height 30
	cull_each false
	renderer billboard
	sorted false
	local_space false
	iteration_interval 0
	nonvisible_update_timeout 0
	billboard_type perpendicular_common
	common_direction 0 1 0 
	common_up_vector 0 0 0.2

	emitter Point
	{
		angle 0
		colour 1 1 1 1
		colour_range_start 1 1 1 1
		colour_range_end 1 1 1 1
		direction 0 1 0
		emission_rate 12
		position 0 0 0
		velocity 10
		time_to_live 0.3
		duration 0.3
	}

	affector LinearForce
	{
		force_vector 0 0 0
		force_application add
	}

	affector Rotator
	{
		rotation_speed_range_start 960
		rotation_speed_range_end 120
		rotation_range_start 0
		rotation_range_end 360
	}
	affector ColourImage
	{
		image colors_5.png
	}
		affector Scaler
	{
		rate 5
	}
}

