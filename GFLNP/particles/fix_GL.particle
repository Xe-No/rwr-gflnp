	sorted false
	local_space false
	iteration_interval 0
	nonvisible_update_timeout 0
	billboard_rotation_type vertex
	
	emitter Ring
	{
		angle 0
		colour 0.5 0.25 0 0.05
		direction 0 1 0
		emission_rate 60
		position 0 0.8 0
		velocity 0.5
		velocity_min 0.5
		velocity_max 1
		time_to_live 1
		time_to_live_min 0.4
		time_to_live_max 0.6
		duration 0.3
		width 2
		height 2
