particle_system mcx_center_circle_01		
{
	quota 1000
	material mcx_sank_circle_03
	particle_width 0.75
	particle_height 0.75
	renderer billboard
	billboard_type point
	emitter Point
	{
		angle 90
		colour 1 1 1 1
		direction 0 1 0
		emission_rate 100
		position 0 0 0
		velocity 0.0001
		time_to_live 0.5
		duration 0.1
	}
	affector ColourFader2
	{
		red1	0
		green1  0
		blue1	0
		alpha1	0
		red2	 -5
		green2  -5
		blue2	-5
		alpha2	-5
		state_change 0.1
	}
	affector Scaler
	{
		rate 1.5
	}
}
