material mcx_sank_circle_03	{
	receive_shadows off
	technique{
		pass{
			lighting off
			fog_override true
			depth_write off
			scene_blend add
            texture_unit{
			tex_address_mode border
            tex_border_colour 0 0 0 0
			texture sank_circle_03.png}
		}
	}
}
