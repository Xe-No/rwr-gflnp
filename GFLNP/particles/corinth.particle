particle_system corinth_blast_spark
{
	quota 800
	emit_emitter_quota 3
	material monlit
	particle_width 0.25
	particle_height 6
	cull_each false

	renderer billboard
                billboard_origin top_center
	billboard_type oriented_self
	billboard_rotation_type texcoord

	sorted false
	local_space false
	iteration_interval 0
	nonvisible_update_timeout 0

	emitter Point
	{
		angle 165.6
		colour 0.72 0.5 0.35 0
		colour_range_start 0.72 0.4 0.35 0
		colour_range_end 0.72 0.5 0.35 0
		direction 0 1 0
		up 0 0 -1
		emission_rate 500
		position 0 0 0
		velocity 6
		velocity_min 5
		velocity_max 6
		time_to_live 0.1
		time_to_live_min 0.1
		time_to_live_max 0.1
		duration 0.05


	}

	affector Scaler
	{
		rate -1
	}
}

