particle_system corinth_fly_by
{
	quota	500
	material corinth_all/corinth_blast_halo
	particle_width	0.6
	particle_height	0.6
	cull_each	false
	renderer	billboard
	billboard_type	point
	billboard_rotation_type vertex

	emitter Point
	{
		angle	0
		colour	1 1 1 1
		colour_range_start	1 1 1 1
		colour_range_end	1 1 1 1
		direction	0 1 0
		emission_rate	50
		position	0 0 0
		velocity	0.0
		velocity_min	0.0
		velocity_max	0.1
		time_to_live	0.05
		time_to_live_min	0.05
		time_to_live_max	0.05
		duration	0.5
	}

	affector Scaler
	{
		rate	0.5
	}

	affector Rotator
	{
		rotation_speed_range_start	-45
		rotation_speed_range_end	45
		rotation_range_start	0
		rotation_range_end	360
	}

	affector LinearForce
	{
		force_vector 0 1 0
		force_application add
	}
}

