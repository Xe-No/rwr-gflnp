particle_system ShadowHeliFlyby
{
	quota	50
	// RWR will internally extend shadow material names with Simple/Integrated
	material	ShadowHeli
	particle_width	15
	particle_height	15
	cull_each	false
	renderer	billboard
	billboard_type	perpendicular_common
	common_direction 0 1 0
	common_up_vector 1 0 0

	emitter Point
	{
		angle	0

		colour	1 1 1 1
		colour_range_start	1 1 1 1
		colour_range_end	1 1 1 1

		// set by RWR
		direction	0 0 0

		position	-100 50 0

		emission_rate	1
		velocity	15.0
		time_to_live	10.0
		duration	0.99

		//repeat_delay    1.0
	}

}


